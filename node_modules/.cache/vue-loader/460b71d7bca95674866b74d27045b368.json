>\n                        <button @click=\"scrollElement('salinity')\">塩分濃度</button>\n                        <button @click=\"scrollElement('water_temp')\">水温</button>\n                        <button @click=\"scrollElement('turbidity')\">濁度</button>\n                    </div>\n            </div>\n        </div>\n        <ChartTest/>\n        <GoPagetop/>\n    </div>\n</template>\n\n<script>\nimport {Component,Vue} from 'vue-property-decorator';\nimport AxiosTest from '@/components/ AxiosTest.vue';\nimport ChartTest from '@/components/ChartTest.vue';\nimport Datepicker from 'vuejs-datepicker';\nimport GoPagetop from '@/components/GoPagetop.vue';\nimport { ja } from 'vuejs-datepicker/dist/locale';\n\n@Component({\n    components:{\n        AxiosTest,\n        ChartTest,\n        Datepicker,\n        GoPagetop,\n    }\n})\nexport default class GetData extends Vue{\n\n    date=new Date();\n    language=ja;\n    text=\"\";\n    axios_test=new AxiosTest();\n    chart_test=new ChartTest();\n    state=\"日付を選択後グラフを生成\";\n\n    mounted(){\n        console.log(\"ページが読み込まれました\");\n        this.plot_data();\n    }\n    \n    async plot_data(){\n        if(this.chart_test.Oxygen || this.chart_test.Chloro || this.chart_test.Salinity || this.chart_test.Water_temp || this.chart_test.Turbidity){\n            this.chart_test.Oxygen.destroy();\n            this.chart_test.Chloro.destroy();\n            this.chart_test.Salinity.destroy();\n            this.chart_test.Water_temp.destroy();\n            this.chart_test.Turbidity.destroy();\n            //this.chart_test.Oxygen_week.destroy();\n        }\n        console.log(\"clicked!\");\n        this.state = \"生成中\";\n        await this.axios_test.get_data();\n        let extract_data=this.axios_test.day_data;\n        this.chart_test.plot_day_data(this.axios_test.day_data);\n        this.chart_test.plot_week_data(this.axios_test.week_12_data);\n        this.state = \"日付を選択後グラフを生成\";\n    }\n\n   \n    onDateSelected(date){\n        this.axios_test.date=date;\n        console.log(date);\n        this.plot_data();\n    }\n\n    scrollElement(name){\n        const target = document.getElementById(name);\n        target.scrollIntoView({behavior: \"smooth\", block: \"start\"});\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n.wrapper1{\n    padding: 0 100px 0 100px;\n    display: flex;\n    justify-content: space-between;\n    .what{\n        width: 50%;\n        margin-left: 30px;\n        .btns{\n            display: flex;\n            flex-wrap: wrap;\n\n            button{\n                display: block;\n                padding: 9px 0;\n                width: 60%;\n                margin: 0 auto 16px auto;\n            }\n        }\n    }\n    .calendar{\n        width: 50%;\n        .vdp-datepicker{\n        display: block;\n        text-align: center;\n    }\n    }\n    \n    \n}\n</style>"]}]}