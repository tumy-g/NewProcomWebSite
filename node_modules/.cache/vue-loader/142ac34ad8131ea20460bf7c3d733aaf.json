                     <button @click=\"scrollElement('chloro')\">クロロフィル</button>\n                        <button @click=\"scrollElement('salinity')\">塩分濃度</button>\n                        <button @click=\"scrollElement('water_temp')\">水温</button>\n                        <button @click=\"scrollElement('turbidity')\">濁度</button>\n                    </div>\n                    <div class=\"btns-sh\">\n                        <button @click=\"scrollElement('oxygen')\">溶存<br>酸素</button>\n                        <button @click=\"scrollElement('chloro')\">クロロフ</button>\n                        <button @click=\"scrollElement('salinity')\">塩分<br>濃度</button>\n                        <button @click=\"scrollElement('water_temp')\">水温</button>\n                        <button @click=\"scrollElement('turbidity')\">濁度</button>\n                    </div>\n            </div>\n        </div>\n        <ChartTest/>\n        <GoPagetop/>\n    </div>\n</template>\n\n<script>\nimport {Component,Vue} from 'vue-property-decorator';\nimport AxiosTest from '@/components/ AxiosTest.vue';\nimport ChartTest from '@/components/ChartTest.vue';\nimport Datepicker from 'vuejs-datepicker';\nimport GoPagetop from '@/components/GoPagetop.vue';\nimport { ja } from 'vuejs-datepicker/dist/locale';\n\n@Component({\n    components:{\n        AxiosTest,\n        ChartTest,\n        Datepicker,\n        GoPagetop,\n    }\n})\nexport default class GetData extends Vue{\n\n    date=new Date();\n    language=ja;\n    text=\"\";\n    axios_test=new AxiosTest();\n    chart_test=new ChartTest();\n    state=\"日付を選択後グラフを生成\";\n\n    week_label = [];\n    month_label = [];\n\n\n    mounted(){\n        console.log(\"ページが読み込まれました\");\n        this.MakeLabel_week(this.date);\n        this.MakeLabel_month(this.date);\n        this.plot_data();\n    }\n    \n    MakeLabel_week(date){\n        // console.log(date);\n        let lastWeek = new Date(date.getFullYear(), date.getMonth(), date.getDate()-6);\n        // console.log(lastWeek);\n        this.week_label = [];\n        for (let week = lastWeek; week <= date; week.setDate(week.getDate() + 1)){\n            this.week_label.push(new Date(week).getMonth()+1 + \"/\" + new Date(week).getDate());\n        }\n        // console.log(this.week_label);\n    }\n    MakeLabel_month(date){\n        console.log(date);\n        let lastMonth = new Date(date.getFullYear(), date.getMonth(), date.getDate()-29);\n        console.log(lastMonth);\n        this.month_label = [];\n        for (let month = lastMonth; month <= date; month.setDate(month.getDate() + 1)){\n            this.month_label.push(new Date(month).getMonth()+1 + \"/\" + new Date(month).getDate());\n        }\n        console.log(this.week_label);\n    }\n    async plot_data(){\n        // if(this.chart_test.Oxygen || this.chart_test.Chloro || this.chart_test.Salinity || this.chart_test.Water_temp || this.chart_test.Turbidity){\n        //     this.chart_test.Oxygen.destroy();\n        //     this.chart_test.Chloro.destroy();\n        //     this.chart_test.Salinity.destroy();\n        //     this.chart_test.Water_temp.destroy();\n        //     this.chart_test.Turbidity.destroy();\n        //     this.chart_test.Oxygen_week.destroy();\n        // }\n        if(this.chart_test.Oxygen && this.chart_test.Oxygen_week && this.chart_test.Oxygen_month){\n            this.chart_test.Oxygen.destroy();\n            this.chart_test.Chloro.destroy();\n            this.chart_test.Salinity.destroy();\n            this.chart_test.Water_temp.destroy();\n            this.chart_test.Turbidity.destroy();\n            this.chart_test.Oxygen_week.destroy();\n            this.chart_test.Chloro_week.destroy();\n            this.chart_test.Salinity_week.destroy();\n            this.chart_test.Water_temp_week.destroy();\n            this.chart_test.Turbidity_week.destroy();\n            this.chart_test.Oxygen_month.destroy();\n            this.chart_test.Chloro_month.destroy();\n            this.chart_test.Salinity_month.destroy();\n            this.chart_test.Water_temp_month.destroy();\n            this.chart_test.Turbidity_month.destroy();\n        }\n        console.log(\"clicked!\");\n        this.state = \"生成中\";\n        await this.axios_test.get_day_data();\n        this.chart_test.plot_day_data(this.axios_test.day_data);\n        await this.axios_test.get_week_data();\n        this.chart_test.plot_week_data(this.axios_test.week_12_data, this.week_label);\n        await this.axios_test.get_month_data();\n        this.chart_test.plot_month_data(this.axios_test.month_data, this.month_label);\n        this.state = \"日付を選択後グラフを生成\";\n    }\n\n    onDateSelected(date){\n        this.axios_test.date=date;\n        this.plot_data();\n\n        this.MakeLabel_week(date);\n        this.MakeLabel_month(date);\n    }\n\n    scrollElement(name){\n        const target = document.getElementById(name);\n        target.scrollIntoView({behavior: \"smooth\", block: \"start\"});\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\nh2 {\n    @media screen and (max-width: 590px){\n        font-size: 1.3rem;\n    }\n}\n.wrapper1{\n    padding: 0 100px 0 100px;\n    display: flex;\n    justify-content: space-between;\n    @media screen and (max-width: 590px) {\n        padding: 0;\n    }\n    .calendar{\n        width: 50%;\n        @media screen and (max-width: 590px){\n            width: 70%;\n            margin-left: 3%;\n        }\n        .vdp-datepicker{\n            display: block;\n            text-align: center;\n        }\n    }\n    .what{\n        width: 50%;\n        margin-left: 30px;\n        @media screen and (max-width: 590px){\n            width: 20%;\n        }\n        h4{\n            @media screen and (max-width: 590px){\n                display: none;\n            }\n        }\n        .btns-pc{\n            @media screen and (max-width: 590px){\n                display: none;\n            }\n            button{\n                display: block;\n                padding: 9px 0;\n                width: 60%;\n                margin: 0 auto 16px auto;\n            }\n        }\n        .btns-sh{\n            display: none;\n            @media screen and (max-width: 590px){\n                display: block;\n            }\n            button{\n                width: 55px;\n                height: 55px;\n                border-radius: 50%;\n                margin-bottom: 17px;\n            }\n        }\n    }\n}\n</style>"]}]}