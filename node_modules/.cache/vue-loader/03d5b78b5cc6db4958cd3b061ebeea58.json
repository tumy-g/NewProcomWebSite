 class=\"about\">\n    <section id=\"hoge\">\n      <div class=\"content one\" @click=\"Trans0to1\" >\n        <h4>グループ１</h4>\n        <p>とり貝数：{{ shellnum }}</p>\n        <p>平均サイズ：{{ size_avg }}</p>\n        <p>コンテナ数：{{ connum }}</p>\n        <p>水深：{{ deep }}</p>\n        <p>更新日：{{ updata }}</p>\n      </div>\n      <div class=\"content tow\" @click=\"Trans0to1\">\n        <h4>グループ2</h4>\n        <p>とり貝数：{{ shellnum }}</p>\n        <p>平均サイズ：{{ size_avg }}</p>\n        <p>コンテナ数：{{ connum }}</p>\n        <p>水深：{{ deep }}</p>\n        <p>更新日：{{ updata }}</p>\n      </div>\n      <div class=\"content thr\" @click=\"Trans0to1\">\n        <h4>グループ3</h4>\n        <p>とり貝数：{{ shellnum }}</p>\n        <p>平均サイズ：{{ size_avg }}</p>\n        <p>コンテナ数：{{ connum }}</p>\n        <p>水深：{{ deep }}</p>\n        <p>更新日：{{ updata }}</p>\n      </div>\n    </section>\n\n    <section id=\"hoge-con\">\n\n      <div id=\"step1\">\n        <div class=\"popup-content one\" v-if=\"Step1on\">手順１\n          <h4>グループ１</h4>\n          <p>とり貝数：{{ shellnum }}</p>\n          <p>平均サイズ：{{ size_avg }}</p>\n          \n          <a href=\"#\" class=\"sikaku\" @click=\"Trans1to2\">コンテナ操作</a>\n          <a href=\"#\" class=\"sikaku\">死亡数入力</a>\n          <a href=\"#\" class=\"sikaku\">サイズ変更</a>\n        </div>\n      </div>\n\n      <div id=\"step2\">\n        <div class=\"popup-content one\" v-if=\"Step2on\">手順２\n          <h4>残りのとり貝</h4>\n          <h5>{{ remain }}</h5>\n          <p>コンテナ1に何個移しますか</p>\n          <a href=\"#\" class=\"sikaku\">とり貝の個数を入力</a>\n          <div class=\"btn-box\">\n            <a href=\"#\" @click=\"Prev2to1\">やり直し</a>\n            <a href=\"#\" @click=\"Trans2to3\">次へ</a>\n          </div>\n        </div>\n      </div>\n\n      <div id=\"step3\">\n        <div class=\"popup-content one\" v-if=\"Step3on\">手順３\n          <h4>以下の内容でコンテナに分割します</h4>\n          <ul>\n            <li>1箱目:{{ shellnum }}</li>\n            <li>2箱目:{{ 0 }}</li>\n          </ul>\n          <div class=\"btn-box\">\n            <a href=\"#\" @click=\"Prev3to2\">やり直し</a>\n            <a href=\"#\" @click=\"Trans3to4\">確定</a>\n          </div>\n        </div>\n      </div>\n\n      <div id=\"step4\">\n        <div class=\"popup-content\" v-if=\"Step4on\">手順４\n          <h4>音声入力</h4>\n          <textarea class=\"record-text\" v-model=\"recoredTexts\"></textarea>\n          <button class=\"record-btn\" @click=\"record\">{{ recordBtnValue }}</button>\n          <!--新しく終了ボタンとやり直しボタンを追加-->\n          <div class=\"btn-box\">\n            <a href=\"#\" @click=\"Prev4to3\">やり直し</a>\n            <a href=\"#\" @click=\"reset_data\">リセット</a>\n            <a href=\"#\" @click=\"DeletePopup(true)\">確定</a>\n          </div>\n        </div>\n      </div>\n    </section>\n    <section class=\"hide\" v-if=\"DeleteItem\" @click=\"DeletePopup(false)\"></section>\n  </div>\n</template>\n\n<script lang=\"ts\">\n\n  export default {\n    data() {\n      return {\n        recognizing: false,\n        recognition: new (window.webkitSpeechRecognition || window.SpeechRecognition)(),\n        recordBtnValue :\"録音開始\",\n        recoredTexts: \"\",\n        recoredMiniText: \"\",\n        size_text: \"\" as string,\n\n        text : \"100\",\n        shellnum: 100,\n        sizes:[] as number[],\n        size_avg:21,\n        remain:0,\n        connum:2,\n        boxes:2,\n        deep:8.5,\n        updata:\"2023/07/05/18:27\",\n\n        Step1on: false,\n        Step2on: false,\n        Step3on: false,\n        Step4on: false,\n\n        DeleteItem: false,\n      };\n    },\n    methods: {\n      async created() {\n        this.recognition.lang = \"ja-JP\";\n        this.recognition.continuous = true;\n        this.recognition.onresult = await this.recognize;\n      },\n\n      async recognize(e: SpeechRecognitionEvent) {\n        this.recoredMiniText += `${e.results[e.results.length - 1][0].transcript}\\n`;\n        if(this.recoredMiniText.includes(\"cm\") || this.recoredMiniText.includes(\"センチ\")){\n          this.recoredTexts+=`${e.results[e.results.length - 1][0].transcript}\\n`;\n          // 正規表現\n          this.split_string(e.results[e.results.length - 1][0].transcript.split(\".\"));\n          this.recoredMiniText=\"\";\n        }else if(this.recoredMiniText.includes(\"CM\")){\n          this.recoredTexts+=`${e.results[e.results.length - 1][0].transcript}\\n`.replace(\"CM\", \"cm\");\n          // 正規表現\n          this.split_string(e.results[e.results.length - 1][0].transcript.split(\".\"));\n          this.recoredMiniText=\"\";\n        }else if(this.recoredMiniText.includes(\"トップ\")){\n          this.recognition.stop();\n          this.recordBtnValue =\"録音開始\";\n          console.log(\"you said\",this.recoredMiniText)\n          this.recoredMiniText=\"\";\n        }else{\n          console.log(this.recoredMiniText);\n        }\n      },\n      // 小数データへの変換\n      split_string(txt_array:Array<string>){\n        txt_array.forEach((element,index) =>{\n          this.size_text+=element.replace(/[^0-9]/g,\"\");\n          if(index===0){\n            this.size_text+=\".\";\n          }\n        });\n        this.sizes.push(parseFloat(this.size_text));\n        this.size_text=\"\";\n      },\n      //　計測中のサイズ、テキストデータをリセット\n      reset_data(){\n        this.size_text=\"\";\n        this.sizes=[];\n        this.recoredTexts=\"\";\n      },\n      record() {\n        this.created();\n        if (this.recognizing) {\n          this.recognition.stop();\n          this.recordBtnValue = \"録音開始\";\n        } else {\n          this.recognition.start();\n          this.recordBtnValue = \"停止\";\n        }\n        this.recognizing = !this.recognizing;\n      },\n      Trans0to1(){\n        this.Step1on = true;\n        this.DeleteItem = true;\n      },\n      Trans1to2(){\n        this.Step2on = true;\n        this.Step1on = false;\n      },\n      Trans2to3(){\n        this.Step3on = true;\n        this.Step2on = false;\n      },\n      Trans3to4(){\n        this.Step4on = true;\n        this.Step3on = false;\n      },\n      Prev2to1(){\n        this.Step2on = false;\n        this.Step1on = true;\n      },\n      Prev3to2(){\n        this.Step3on = false;\n        this.Step2on = true;\n      },\n      Prev4to3(){\n        this.Step4on = false;\n        this.Step3on = true;\n      },\n      // 日付を更新する\n      update_day(){\n        var date = new Date();\n        this.updata = date.getFullYear()\n          + '/' + ('0' + (date.getMonth() + 1)).slice(-2)\n          + '/' + ('0' + date.getDate()).slice(-2)\n          + ' ' + ('0' + date.getHours()).slice(-2)\n          + ':' + ('0' + date.getMinutes()).slice(-2)\n          + ':' + ('0' + date.getSeconds()).slice(-2)\n        console.log(this.updata); \n      },\n      DeletePopup(change:boolean){\n        // データの更新を行うのであれば、if文を実行\n        if(change){\n          try{\n            console.log(\"value changed\");\n            this.size_avg=this.sizes.reduce((a,b)=>{\n              return a+b;\n            });\n            var size_avg_test=this.sizes.length;\n            // 計算ができなかったら例外を発生させる\n            if(isNaN(size_avg_test)){\n              throw \"データが正しく入力されませんでした。\"\n            }else{\n              this.size_avg/=this.sizes.length;\n              this.update_day();\n              console.log(this.size_avg);\n            }\n          }catch{\n            alert!(\"データの変更に失敗しました。\\nもう一度入力してください\");\n            return\n          }\n        }\n        this.Step1on = false;\n        this.Step2on = false;\n        this.Step3on = false;\n        this.Step4on = false;\n        this.DeleteItem = false;\n      },\n    },\n  };\n\n</script>\n\n<style lang=\"scss\" scoped>\n.popup{\n  background-color: aqua;\n}\nul{\n  padding-left: 0;\n  li {\n    list-style: none;\n  }\n}\na {\n  text-decoration: none;\n  color: #444;\n}\n.about {\n  background-image: url(\"https://min-chi.material.jp/mc/materials/background-c/sea_island/sea_island_1.jpg\");\n  height: 100vh;\n  width: 95vw;\n  border-radius: 5%;\n  background-size: cover;\n  margin: 0 auto;\n  #hoge {\n  display: flex;\n  justify-content: space-around;\n  background-size: cover;\n  padding-top: 100px;\n    .content {\n      width: 150px;\n      height: 300px;\n      background-color: black;\n      border-radius: 5%;\n      color: white;\n        button {\n          display: block;\n        }\n      }\n  }\n  #hoge-con {\n    margin: 0 auto;\n    .popup-content{\n        width: 300px;\n        height: 300px;\n        background-color: rgb(0, 200, 0);\n        border-radius: 30%;\n        margin-top: 100px;\n        font-size: 1rem;\n        font-weight: bold;\n        box-shadow: 10px 10px 5px #888888;\n        position: fixed;\n        top: 50%;\n        left: 50%;\n        transform: translate(-50%,-50%);\n        z-index: 1;\n        .btn-box{\n          display: flex;\n          justify-content: space-around;\n          a {\n            background-color: white;\n            display: block;\n            border-radius: 50%;\n            width: 30%;\n            margin: 0 auto 20px auto;\n            height: 60px;\n            padding-top: 10%;\n          }\n        }\n        .sikaku{\n          display: block;\n          border: 1px solid white;\n          background-color: white;\n          margin : 0 auto 10px auto;\n          width: 80%;\n          border-radius: 5%;\n        }\n        .record-text{\n          width: 200px;\n          height: 100px;\n          font-size: 30%;\n        }\n        .record-btn{\n          background-color: white;\n          width: 100px;\n          height: 40px;\n          font-size: 50%;\n          border-radius: 10%;\n        }\n    }\n    #step2{\n      h5{\n        background-color: white;\n        width: 10%;\n        height: 10%;\n        display: inline;\n      }\n    }\n    #step3{\n      li{\n        background-color: white;\n        width: 50%;\n        margin: 0 auto 15px auto;\n        border-radius: 10%;\n      }\n    }\n  }\n  .hide{\n    background-color: rgba($color: #444, $alpha: 0.5);\n    position: fixed;\n    top: 0px;\n    left: 0px;\n    height: 100vh;\n    width: 100vw;\n    z-index: 0;\n  }\n}\n</style>"]}]}