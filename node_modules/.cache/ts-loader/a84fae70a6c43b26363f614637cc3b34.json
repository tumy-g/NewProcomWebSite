MiniText.includes(\"CM\")){\n          this.recoredTexts+=`${e.results[e.results.length - 1][0].transcript}\\n`.replace(\"CM\", \"cm\");\n          // 正規表現\n          this.split_string(e.results[e.results.length - 1][0].transcript.split(\".\"));\n          this.recoredMiniText=\"\";\n        }else if(this.recoredMiniText.includes(\"トップ\")){\n          this.recognition.stop();\n          this.recordBtnValue =\"録音開始\";\n          console.log(\"you said\",this.recoredMiniText)\n          this.recoredMiniText=\"\";\n        }else{\n          console.log(this.recoredMiniText);\n        }\n      },\n      // 小数データへの変換\n      split_string(txt_array:Array<string>){\n        txt_array.forEach((element,index) =>{\n          this.size_text+=element.replace(/[^0-9]/g,\"\");\n          if(index===0){\n            this.size_text+=\".\";\n          }\n        });\n        this.sizes.push(parseFloat(this.size_text));\n        this.size_text=\"\";\n      },\n      //　計測中のサイズ、テキストデータをリセット\n      reset_data(){\n        this.size_text=\"\";\n        this.sizes=[];\n        this.recoredTexts=\"\";\n      },\n      record() {\n        this.created();\n        if (this.recognizing) {\n          this.recognition.stop();\n          this.recordBtnValue = \"録音開始\";\n        } else {\n          this.recognition.start();\n          this.recordBtnValue = \"停止\";\n        }\n        this.recognizing = !this.recognizing;\n      },\n      Trans0to1(){\n        this.Step1on = true;\n        this.DeleteItem = true;\n      },\n      Trans1to2(){\n        this.Step2on = true;\n        this.Step1on = false;\n      },\n      Trans2to3(){\n        this.Step3on = true;\n        this.Step2on = false;\n      },\n      Trans3to4(){\n        this.Step4on = true;\n        this.Step3on = false;\n      },\n      Prev2to1(){\n        this.Step2on = false;\n        this.Step1on = true;\n      },\n      Prev3to2(){\n        this.Step3on = false;\n        this.Step2on = true;\n      },\n      Prev4to3(){\n        this.Step4on = false;\n        this.Step3on = true;\n      },\n      // 日付を更新する\n      update_day(){\n        var date = new Date();\n        this.updata = date.getFullYear()\n          + '/' + ('0' + (date.getMonth() + 1)).slice(-2)\n          + '/' + ('0' + date.getDate()).slice(-2)\n          + ' ' + ('0' + date.getHours()).slice(-2)\n          + ':' + ('0' + date.getMinutes()).slice(-2)\n        console.log(this.updata); \n      },\n      DeletePopup(change:boolean){\n        // データの更新を行うのであれば、if文を実行\n        if(change){\n          try{\n            console.log(\"value changed\");\n            this.size_avg=this.sizes.reduce((a,b)=>{\n              return a+b;\n            });\n            var size_avg_test=this.sizes.length;\n            // 計算ができなかったら例外を発生させる\n            if(isNaN(size_avg_test)){\n              throw \"データが正しく入力されませんでした。\"\n            }else{\n              this.size_avg/=this.sizes.length;\n              // 日付更新はここで\n              this.update_day();\n              console.log(this.size_avg);\n            }\n          }catch{\n            alert!(\"データの変更に失敗しました。\\nもう一度入力してください\");\n            return\n          }\n        }\n        this.Step1on = false;\n        this.Step2on = false;\n        this.Step3on = false;\n        this.Step4on = false;\n        this.DeleteItem = false;\n        this.reset_data();\n      },\n    },\n  });\n"],"sourceRoot":""}]}