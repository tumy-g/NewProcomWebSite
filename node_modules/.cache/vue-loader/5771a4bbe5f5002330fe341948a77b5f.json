tepicker from 'vuejs-datepicker';\nimport { ja } from 'vuejs-datepicker/dist/locale';\n\n@Component({\n    components:{\n        AxiosTest,\n        ChartTest,\n        Datepicker,\n    }\n})\nexport default class GetData extends Vue{\n\n    date=new Date();\n    language=ja;\n    text=\"\";\n    axios_test=new AxiosTest();\n    chart_test=new ChartTest();\n\n    async plot_data(){\n        if(this.chart_test.Oxygen || this.chart_test.Chloro || this.chart_test.Salinity || this.chart_test.Water_temp || this.chart_test.Turbidity){\n            this.chart_test.Oxygen.destroy();\n            this.chart_test.Chloro.destroy();\n            this.chart_test.Salinity.destroy();\n            this.chart_test.Water_temp.destroy();\n            this.chart_test.Turbidity.destroy();\n        }\n        console.log(\"clicked!\");\n        let extract_data=await this.axios_test.get_data();\n        this.chart_test.plot_data(extract_data);\n    }\n\n    onDateSelected(date){\n        this.text=date.toISOString().slice(0, 10); \n        this.axios_test.text=this.text;\n        console.log(this.text);\n    }\n}\n</script>\n<style lang=\"scss\">\n.wrapper1{\n    display: flex;\n    padding: 0 100px 0 100px;\n    button{\n        width: 100px;\n        height: 50px;\n        border-radius: 10%;\n        display: block;\n    }\n}\n</style>"]}]}