                    });\n            this.oxygen_day_6m = [...Array<number>(24)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.day_data[6][index].oxygen);\n                        });\n            this.oxygen_day_9m = [...Array<number>(24)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.day_data[9][index].oxygen);\n                        });\n            this.oxygen_day_11m = [...Array<number>(24)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.day_data[11][index].oxygen);\n                        });\n            console.log(this.oxygen_day_1m);\n        },\n        async load_data_week(){\n            await this.axios.get_week_data();\n            this.oxygen_week_1m = [...Array<number>(7)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.week_12_data[1][index].oxygen);\n                        });\n            this.oxygen_week_3m = [...Array<number>(7)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.week_12_data[3][index].oxygen);\n                        });\n            this.oxygen_week_6m = [...Array<number>(7)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.week_12_data[6][index].oxygen);\n                        });\n            this.oxygen_week_9m = [...Array<number>(7)].map((_,index)=>{\n                            // extract_dataは配列でなく、整数キー（水深）の連想配列\n                            return Number(this.axios.week_12_data[9][index].oxygen);\n                        });\n            this.oxygen_week_11m = [...Array<number>(7)].map((_,index